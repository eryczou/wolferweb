{
  "name": "wolfernode",
  "version": "1.0.0",
  "description": "Redux + React Web Server for WolferX",
  "main": "main.js",
  "author": "WolferX",
  "license": "MIT",
  "keywords": [
    "Node",
    "Redux",
    "React",
    "Webpack"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rm -rf public",
    "compile": "better-npm-run compile",
    "start": "better-npm-run start",
    "forever": "better-npm-run forever",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "debug": "better-npm-run debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "flow:check": "babel-node bin/flow-check",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "debug": {
      "command": "nodemon --exec babel-node --debug bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "forever": {
      "command": "forever start -c node_modules/.bin/babel-node bin/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start bin/karma.js",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wolferian/wolferweb.git"
  },
  "dependencies": {
    "async": "^1.5.1",
    "axios": "^0.9.1",
